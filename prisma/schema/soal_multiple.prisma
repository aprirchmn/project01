model soal_multiple {
  id_soal_multiple  Int            @id @default(autoincrement())
  id_mata_pelajaran Int
  id_jenis_ujian    Int
  pertanyaan        String
  pilihan_a         String
  pilihan_b         String
  pilihan_c         String
  pilihan_d         String
  pilihan_e         String
  kunci_jawaban     String
  bobot             Float
  jawaban           jawaban[]
  jenis_ujian       jenis_ujian    @relation(fields: [id_jenis_ujian], references: [id_jenis_ujian])
  mata_pelajaran    mata_pelajaran @relation(fields: [id_mata_pelajaran], references: [id_mata_pelajaran])
}